{
  "name": "arthen-lang",
  "version": "1.0.0",
  "description": "ARTHEN - AI-Native Programming Language for Blockchain Ecosystems",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "compile": "node scripts/compile.js",
    "test": "node scripts/test.js",
    "deploy": "node scripts/deploy.js",
    "dev": "node scripts/dev.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docs": "node scripts/generate-docs.js",
    "benchmark": "node scripts/benchmark.js",
    "postinstall": "node scripts/postinstall_setup_icons.cjs"
  },
  "keywords": [
    "blockchain",
    "ai",
    "programming-language",
    "smart-contracts",
    "cross-chain",
    "ml-consensus",
    "defi",
    "web3"
  ],
  "author": "ARTHEN Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rafael2022-prog/arthen-lang.git"
  },
  "bugs": {
    "url": "https://github.com/Rafael2022-prog/arthen-lang/issues"
  },
  "homepage": "https://arthen-lang.org",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "web3": "^4.0.0",
    "@solana/web3.js": "^1.87.0",
    "@cosmjs/stargate": "^0.32.0",
    "@polkadot/api": "^10.0.0",
    "near-api-js": "^2.1.0",
    "@layerzerolabs/lz-sdk": "^2.0.0",
    "axios": "^1.6.0",
    "ethers": "^6.8.0",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "pkg": "^5.8.1"
  },
  "peerDependencies": {
    "python": ">=3.8.0"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "transformers": "^2.0.0"
  },
  "config": {
    "arthen": {
      "compiler_path": "./compiler/arthen-compiler.py",
      "stdlib_path": "./stdlib/",
      "examples_path": "./examples/",
      "build_path": "./build/"
    }
  },
  "bin": {
    "arthen": "./bin/arthen.js"
  },
  "files": [
    "src/",
    "compiler/",
    "stdlib/",
    "examples/",
    "bin/",
    "scripts/",
    "assets/",
    "README.md",
    "LICENSE",
    "arthen.config.json"
  ],
  "pkg": {
    "assets": [
      "assets/icons/**/*",
      "scripts/**/*",
      "editors/vscode/arthen-file-icons/**/*"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}